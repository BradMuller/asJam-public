package io.spaceport.ui {		import flash.display.MovieClip;	import flash.display.DisplayObject;		public class TabbedPanel extends MovieClip {				private var panels:Object = {};		private var selectedPanel:DisplayObject = null;				public function TabbedPanel() {			// constructor code			super();			return;						for( var i = 0; i < this.numChildren; i++ ){				var child:DisplayObject = this.getChildAt( i );				var matchData = child.name.match( /^(.*)Tab$/ );				if( matchData ){					wrapButton( matchData[1], child );				}			}					}				private function wrapButton( tabName:String, tab:DisplayObject ){			var panel:DisplayObject = this.getChildByName( tabName + "Panel" );						panels[ tabName ] = panel;						tab.addEventListener( "buttonPressed", function(event){				trace("here");				selectedPanel.visible = false;				panel.visible = true;				selectedPanel = panel;			});						if( selectedPanel == null ){				selectedPanel = panel;				panel.visible = true;			}else{				panel.visible = false;			}		}	}	}